<p id="notice"><%= notice %></p>

<% @events.each do |event| %>
  <h1><%= event.name %></h1>

  <p>
    <%= image_tag event.image.url(:thumb) %>
  </p>

  <p>
    <strong>When:</strong>
    <%= event.event_date %>
  </p>

  <p>
    <strong>Where:</strong>
    <%= event.address %>
  </p>
<% end %>  

